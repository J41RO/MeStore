CORS_ORIGINS=https://mestocker.com,https://www.mestocker.com,https://app.mestocker.com
CORS_ALLOW_HEADERS=Authorization,Content-Type,Accept,X-Requested-With,Cache-Control,X-API-Key
CORS_ALLOW_METHODS=GET,POST,PUT,DELETE

# Database Configuration for Production
ENVIRONMENT=production
DATABASE_URL=postgresql+asyncpg://mestocker_user:${DB_PASSWORD}@${DB_HOST}:${DB_PORT}/${DB_NAME}
DB_HOST=localhost
DB_PORT=5432
DB_NAME=mestocker_prod
DB_USER=mestocker_user
DB_PASSWORD=production_secure_password_change_me

# Logging Configuration for Production
LOG_LEVEL=WARNING