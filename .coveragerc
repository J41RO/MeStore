[run]
source = app
omit = 
    app/__init__.py
    app/middleware/__init__.py
    app/models/__init__.py
    app/core/__init__.py
    app/api/__init__.py
    */tests/*
    */__pycache__/*
    .venv/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    
skip_covered = false
precision = 2
show_missing = true
