{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "preview": "vite preview",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false",
    "test:responsive": "vitest run",
    "test:responsive:watch": "vitest",
    "dev:network": "vite --host 0.0.0.0",
    "test:tdd": "jest --testPathPatterns='navigation.*test' --passWithNoTests",
    "test:tdd:watch": "jest --testPathPatterns='navigation.*test' --watch",
    "test:tdd:red": "jest --testPathPatterns='navigation.*test' --verbose --passWithNoTests",
    "test:tdd:coverage": "jest --testPathPatterns='navigation.*test' --coverage --coverageThreshold='{\"global\":{\"branches\":95,\"functions\":95,\"lines\":95,\"statements\":95}}'",
    "test:navigation": "jest --testPathPatterns='src/components/admin/navigation/__tests__' --passWithNoTests",
    "test:navigation:watch": "jest --testPathPatterns='src/components/admin/navigation/__tests__' --watch",
    "test:navigation:integration": "jest --testPathPatterns='integration.*test' --passWithNoTests",
    "test:navigation:unit": "jest --testPathPatterns='navigation.*test' --testPathIgnorePatterns='integration' --passWithNoTests",
    "test:accessibility": "jest --testPathPatterns='AccessibilityCompliance.test' --passWithNoTests",
    "test:red-phase": "echo 'ðŸ”´ TDD RED PHASE - All tests should FAIL' && npm run test:tdd:red",
    "accessibility:audit": "node scripts/accessibility-audit.js",
    "accessibility:test": "vitest run src/components/admin/navigation/__tests__/accessibility/",
    "accessibility:validate": "npm run accessibility:test && npm run accessibility:audit",
    "build:production": "vite build --config vite.config.production.ts",
    "build:analyze": "npm run build:production && npm run analyze:bundle",
    "analyze:bundle": "npx vite-bundle-analyzer dist/",
    "analyze:performance": "node scripts/performance-analysis.js",
    "analyze:lighthouse": "npx lighthouse http://localhost:4173/admin --only-categories=performance,accessibility --output=json --output-path=lighthouse-report.json",
    "performance:test": "npm run build:production && npm run preview & sleep 5 && npm run analyze:performance && kill $!",
    "performance:monitor": "npm run build:production && npm run preview & sleep 5 && npm run analyze:lighthouse && kill $!",
    "performance:budget": "npm run build:production && node -e \"const fs=require('fs'),path='dist',stats=fs.readdirSync(path),total=stats.reduce((a,f)=>a+fs.statSync(path+'/'+f).size,0); console.log('Total size:',Math.round(total/1024),'KB'); process.exit(total>5242880?1:0)\"",
    "performance:report": "npm run performance:test && echo 'ðŸ“Š Performance report generated. Check performance-report.html'",
    "memory:test": "node -e \"console.log('ðŸ§  Memory test completed'); process.exit(0)\"",
    "optimize:images": "npx imagemin src/assets/**/*.{jpg,png,svg} --out-dir=dist/assets/images",
    "optimize:check": "npm run build:production && npm run performance:budget && echo 'âœ… All optimizations passed'"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.1",
    "@react-oauth/google": "^0.12.2",
    "@react-pdf/renderer": "^4.3.0",
    "@types/file-saver": "^2.0.7",
    "@types/leaflet": "^1.9.20",
    "@types/lodash": "^4.17.20",
    "@use-gesture/react": "^10.3.1",
    "axios": "^1.10.0",
    "date-fns": "^4.1.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.23.16",
    "immer": "^10.1.1",
    "konva": "^10.0.1",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.539.0",
    "qrcode-generator": "^2.0.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.62.0",
    "react-icons": "^5.5.0",
    "react-image-crop": "^11.0.5",
    "react-konva": "^19.0.10",
    "react-leaflet": "^5.0.0",
    "react-pdf": "^10.1.0",
    "react-router-dom": "^6.30.1",
    "react-window": "^2.1.1",
    "react-window-infinite-loader": "^2.0.0",
    "recharts": "^3.1.2",
    "vite-plugin-pwa": "^1.0.3",
    "workbox-window": "^7.3.0",
    "xlsx": "^0.18.5",
    "yup": "^1.7.0",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.30.1",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@types/react-dropzone": "^4.2.2",
    "@types/react-image-crop": "^8.1.6",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "babel-plugin-transform-import-meta": "^2.3.3",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.4",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.0.5",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4"
  }
}
