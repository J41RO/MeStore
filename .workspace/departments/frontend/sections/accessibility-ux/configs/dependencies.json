{
  "accessibility_tools": {
    "testing": {
      "jest-axe": "^10.0.0",
      "@axe-core/react": "^4.10.2",
      "@testing-library/jest-dom": "^6.7.0",
      "@testing-library/react": "^16.3.0",
      "@testing-library/user-event": "^14.6.1"
    },
    "linting": {
      "eslint-plugin-jsx-a11y": "^6.10.2"
    },
    "development": {
      "@types/jest": "^30.0.0"
    }
  },
  "accessibility_libraries": {
    "drag_drop": {
      "@dnd-kit/core": "^6.3.1",
      "@dnd-kit/sortable": "^10.0.0",
      "@dnd-kit/utilities": "^3.2.2"
    },
    "animation": {
      "framer-motion": "^12.23.16"
    },
    "charts": {
      "recharts": "^3.1.2"
    }
  },
  "wcag_compliance": {
    "level": "AA",
    "version": "2.1",
    "requirements": {
      "color_contrast": {
        "normal_text": "4.5:1",
        "large_text": "3:1",
        "ui_components": "3:1"
      },
      "touch_targets": {
        "minimum_size": "44px",
        "spacing": "8px"
      },
      "focus_management": {
        "visible_indicators": true,
        "logical_order": true,
        "trap_in_modals": true
      },
      "screen_reader": {
        "semantic_structure": true,
        "aria_labels": true,
        "live_regions": true,
        "landmarks": true
      },
      "keyboard_navigation": {
        "all_interactive_elements": true,
        "skip_links": true,
        "escape_handlers": true
      }
    }
  },
  "testing_strategies": {
    "automated": {
      "jest_axe": "Component level WCAG violations",
      "lighthouse": "Page level accessibility audit",
      "eslint_jsx_a11y": "Static code analysis"
    },
    "manual": {
      "screen_readers": ["NVDA", "VoiceOver", "TalkBack"],
      "keyboard_only": "Tab, arrow keys, Enter, Space, Escape",
      "high_contrast": "Windows High Contrast mode",
      "zoom": "200% browser zoom",
      "reduced_motion": "prefers-reduced-motion"
    }
  },
  "implementation_status": {
    "VendorRegistrationFlow": "WCAG 2.1 AA Compliant",
    "VendorAnalyticsOptimized": "WCAG 2.1 AA Compliant",
    "EnhancedProductDashboard": "WCAG 2.1 AA Compliant",
    "AccessibleBarChart": "WCAG 2.1 AA Compliant",
    "AccessiblePieChart": "WCAG 2.1 AA Compliant",
    "useAccessibleDragDrop": "WCAG 2.1 AA Compliant"
  }
}